{
  "displayName": "unit",
  "rootDir": "src",
  "testMatch": ["**/*.spec.ts"],
  "testPathIgnorePatterns": ["<rootDir>/app.controller.spec.ts"],
  "transform": { "^.+\\.(t|j)s$": "ts-jest" },
  "moduleFileExtensions": ["ts", "js", "json"],
  "testEnvironment": "node",
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "../coverage/unit"
}
